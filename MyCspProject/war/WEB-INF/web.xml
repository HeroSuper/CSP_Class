<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  	<servlet>
  	    <servlet-name>SignUp</servlet-name>
  	    <servlet-class>com.ac.company.SignUp</servlet-class>
  	</servlet>
  	<servlet-mapping>
  	    <servlet-name>SignUp</servlet-name>
  	    <url-pattern>/signup</url-pattern>
  	</servlet-mapping>
  	<servlet>
  	    <servlet-name>RegistMember</servlet-name>
  	    <servlet-class>com.ac.company.RegistMember</servlet-class>
  	</servlet>
  	<servlet-mapping>
  	    <servlet-name>RegistMember</servlet-name>
  	    <url-pattern>/registmember</url-pattern>
  	</servlet-mapping>
  	<servlet>
  	    <servlet-name>SelectAllMember</servlet-name>
  	    <servlet-class>com.ac.company.SelectAllMember</servlet-class>
  	</servlet>
  	<servlet-mapping>
  	    <servlet-name>SelectAllMember</servlet-name>
  	    <url-pattern>/allmember</url-pattern>
  	</servlet-mapping>
    

    <welcome-file-list>
        <welcome-file>LoginPage.html</welcome-file>
    </welcome-file-list>
    
</web-app>